{
  "name": "gatsby-plugin-complex-sitemaps",
  "version": "1.0.0",
  "description": "Gatsby plugin for generating multiple sitemaps from datalayer",
  "main": "dist/gatsby-node.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "clean": "rm -rf node_modules",
    "lint": "run-p lint:*",
    "lint:js": "run-p lint:js:*",
    "lint:js:eslint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:js:tsc": "tsc --noEmit --skipLibCheck",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:js": "npm run lint:js:eslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chefclub/gatsby-plugin-complex-sitemaps.git"
  },
  "keywords": [
    "sitemap",
    "complex",
    "chefclub",
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "ChefClub",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chefclub/gatsby-plugin-complex-sitemaps/issues"
  },
  "homepage": "https://github.com/Chefclub/gatsby-plugin-complex-sitemaps#readme",
  "devDependencies": {
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "gatsby": "^3.14.6"
  }
}
